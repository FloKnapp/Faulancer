{
    "name": "floknapp/faulancer",
    "description": "A collection of helpers to kickstart your project",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Flo Knapp",
            "email": "office@florianknapp.de"
        }
    ],
    "autoload": {
      "psr-4": {
        "Faulancer\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Faulancer\\Test\\": "tests/",
        "Faulancer\\Fixture\\": "tests/Fixture/src/"
      }
    },
    "require": {
      "php": "^7.0",
      "tflori/orm": "^1.2"
    },
    "require-dev": {
      "phpunit/phpunit": "^5.6",
      "squizlabs/php_codesniffer": "^3.0"
    },
    "minimum-stability": "beta",
    "scripts": {
      "test": "./vendor/bin/phpunit -c phpunit.xml --colors=always"
    }
}
