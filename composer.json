{
    "name": "floknapp/faulancer",
    "description": "A collection of helpers to kickstart your project",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Flo Knapp",
            "email": "office@florianknapp.de"
        }
    ],
    "autoload": {
      "psr-4": {
        "Faulancer\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Faulancer\\Test\\": "tests/",
        "Faulancer\\Fixture\\": "tests/Fixture/src/"
      }
    },
    "repositories": [
      {
        "type": "path",
        "url": "../../tflori/orm",
        "bin": [ "bin/orm-console" ]
      }
    ],
    "require": {
      "php": "^7.0",
      "tflori/orm": "dev-feature-console"
    },
    "require-dev": {
      "phpunit/phpunit": "^5.6",
      "apigen/apigen": "^4.2@dev"
    },
    "minimum-stability": "alpha"
}
